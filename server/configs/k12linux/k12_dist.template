if [ -z "$K12_DIST" ]; then
    echo "ERROR: K12_DIST not defined in /etc/sysconfig/k12_dist."
    exit 1
fi
case "$K12_DIST" in
    fc8)
        export VENDORDEF="Fedora"
        export RELEASEDEF=8
        export TFTPDIR=/tftpboot
    ;;
    fc9)
        export VENDORDEF="Fedora"
        export RELEASEDEF=9
        export TFTPDIR=/var/lib/tftp
    ;;
    el5)
        export VENDORDEF="EL"
        export RELEASEDEF=5
        export TFTPDIR=/tftpboot
    ;;
    *)
        echo "ERROR: $K12_DIST is unsupported."
        exit 1
    ;;
esac
